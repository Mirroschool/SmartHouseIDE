{
  "name": "ArduBlock",
  "version": "1.0",
  "description": "Blockly is a library for building visual programming editors.",
  "keywords": ["blockly"],
  "scripts": {
    "lint": "jshint ."
  },
  "repository": {
    "type": "git",
    "url": "http://ardublock.ru/"
  },
  "bugs": {
    "url": " "
  },
  "homepage": "http://ardublock.ru/",
  "author": {
    "name": "Vitaliy Petrov"
  },
  "license": "Apache-2.0",
  "private": true,
  "devDependencies": {
    "jshint": "latest"
  },
  "jshintConfig": {
    "globalstrict": true,
    "predef": ["Blockly", "goog", "window", "document", "soy", "XMLHttpRequest"],
    "sub": true,
    "undef": true,
    "unused": true
  }
}
